    "@mui/icons-material": "5.15.5",
    "@mui/lab": "5.0.0-alpha.161",
    "@mui/material": "5.15.5",
    "date-fns": "3.3.0",
    "firebase": "10.7.2",
    "firebaseui": "6.1.0",
    "next": "14.1.0",
    "next-transpile-modules": "10.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.49.3",
    "react-icons": "5.0.1"
  },
