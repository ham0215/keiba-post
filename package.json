    "prettier": "prettier --check \"src/**/*.{ts,tsx}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/cache": "11.11.0",
