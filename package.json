    "@typescript-eslint/parser": "5.45.0",
    "eslint": "8.28.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.6",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "local-ssl-proxy": "^1.3.0",
    "postcss-syntax": "0.36.2",
    "prettier": "2.8.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
